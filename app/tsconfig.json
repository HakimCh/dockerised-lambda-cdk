{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "./dist",
        "rootDir": ".",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "webhooks/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "**/*.d.ts",
        "tsconfig.tsbuildinfo"
    ]
}